<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE beans  PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="regraslist" class="br.unb.cic.lp.gol.estrategias.RegrasList" scope="prototype" >
		<property name="regras">
		<list>
		<ref bean="Conway" />
		<ref bean="HighLife" />
		<ref bean="LiveFreeOrDie" />
		</list>
		</property>
	</bean>
<bean id="Conway"  class="br.unb.cic.lp.gol.estrategias.Conway" scope="prototype"/>
<bean id="HighLife"  class="br.unb.cic.lp.gol.estrategias.HighLife" scope="prototype"/>
<bean id="LiveFreeOrDie"  class="br.unb.cic.lp.gol.estrategias.LiveFreeOrDie" scope="prototype"/>
</beans>
