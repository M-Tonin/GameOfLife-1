<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans  PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
<bean id="conway"  class="br.unb.cic.lp.gol.estrategias.Conway" />
<bean id="highlife"  class="br.unb.cic.lp.gol.estrategias.HighLife" />
<bean id="livefreeordie"  class="br.unb.cic.lp.gol.estrategias.LiveFreeOrDie" />
	<bean id="regraslist" class="br.unb.cic.lp.gol.estrategias.RegrasList" scope="prototype" >
		<property name="EstrategiaDeDerivacao">
		<list>
		<ref bean="conway" />
		<ref bean="highlife" />
		<ref bean="livefreeordie" />
		</list>
		</property>
	</bean>
</beans>
